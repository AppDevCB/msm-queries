 <h1>
  Director #<%=@the_director.id%> details
</h1>

<a href="/directors" class="btn btn-block btn-outline-secondary">
  Go back
</a>

<dl>
  <dt>
    Name
  </dt>
  <dd>
    <%= @the_director.name %>
  </dd>

  <dt>
    Dob
  </dt>
  <dd>
    <%= @the_director.dob %>
  </dd>

  <dt>
    Bio
  </dt>
  <dd>
   <%= @the_director.bio %>
  </dd>

  <dt>
    Image
  </dt>
  <dd>
    <img src="http://robohash.org/24">
  </dd>

  <dt>
    Created at
  </dt>
  <dd>
    <%= time_ago_in_words(@the_director.created_at)%> ago
  </dd>

  <dt>
    Updated at
  </dt>
  <dd>
    <%= time_ago_in_words(@the_director.created_at)%> ago
  </dd>
</dl>

<h1>Filmographies</h1>

<table border ="1">
<%@themovies.each do |element|%>
  <tr>
    <td>
      <%= element.title%>
    </td>
        <td>
      <%= element.year%>
      </td>
<td>
   <img src="http://robohash.org/24">
   </td>

              <td>
      <a href="/movies/<%= element.id%>"> Show details </a>
    </td>

    </tr>
<%end%>
